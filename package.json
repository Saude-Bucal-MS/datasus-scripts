{
  "name": "@saude-bucal-ms/datasus-scripts",
  "version": "0.1.0",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "check": "biome check",
    "check:fix": "biome check --fix",
    "prepare": "husky || true",
    "build": "tsc && tsc-alias",
    "dev": "tsx -r \"dotenv-flow/config\" src/cli.ts",
    "cli": "node -r \"dotenv-flow/config\" dist/cli.js",
    "test": "vitest run ./src",
    "test:coverage": "vitest run ./src --coverage",
    "verify": "run-s check test build",
    "release": "release-it"
  },
  "dependencies": {
    "basic-ftp": "^5.0.5",
    "better-sqlite3": "^12.4.1",
    "commander": "^14.0.1",
    "consola": "3.4.2",
    "dbffile": "^1.12.0",
    "dotenv-flow": "^4.1.0",
    "knex": "^3.1.0",
    "tmp": "^0.2.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@commander-js/extra-typings": "^14.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.3.1",
    "@types/tmp": "^0.2.6",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "release-it": "^19.0.4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "resolutions": {
    "conventional-changelog-conventionalcommits": "8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
